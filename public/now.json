{
  "version": 2,
  "alias": "huberts-journal-app.now.sh",
  "name": "journal-app",
  "routes": [
      {
        "src": "^/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "^/favicon.ico$",
        "dest": "/favicon.ico"
      },
      {
        "src": ".*",
        "dest": "/index.html"
      }
  ]
}